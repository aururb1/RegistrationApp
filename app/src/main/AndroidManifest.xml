<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.urbon.registrationapp">

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@drawable/veterinarian"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.SigninActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.MainActivity" />
        <activity android:name=".activities.CustomersActivity" />
        <activity
            android:name=".activities.InformationActivity"
            android:windowSoftInputMode="stateAlwaysHidden" />
        <activity android:name=".activities.RegisterOwnerActivity" />
        <activity android:name=".activities.RegisterAnimalActivity" />
    </application>

</manifest>